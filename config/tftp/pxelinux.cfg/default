DEFAULT menu.c32
PROMPT 0
TIMEOUT 300
ONTIMEOUT ubuntu

MENU TITLE PXE Boot Menu

LABEL ubuntu
    MENU LABEL Install Ubuntu 22.04 LTS
    KERNEL ubuntu/vmlinuz
    APPEND initrd=ubuntu/initrd.gz url=http://192.168.1.1/ubuntu/preseed.cfg auto=true priority=critical vga=normal --- quiet
    
LABEL local
    MENU LABEL Boot from local disk
    LOCALBOOT 0
